<!-- Page 1 -->
<amp-story-page id="page1" class="amp-story-page amp-story-page__full" >
    
    <amp-story-grid-layer template="fill" class="layer-background">
<amp-img src="assets/images/obama-inaugural.webp"
    width="0.56"
    height="1"
    alt="Barack Obama"
    attribution="The Obama White House"
    layout="fill"
    class="amp-image__overlay amp-image__overlay--full-light  amp-image--blur "> 
    <amp-img src="assets/images/obama-inaugural.jpg"
        fallback
        width="0.56"
        height="1"
        alt="Barack Obama"
        attribution="The Obama White House"
        layout="fill"
        class="amp-image__overlay amp-image__overlay--full-light  amp-image--blur  "></amp-img>
</amp-img>    </amp-story-grid-layer>

    <amp-story-grid-layer template="vertical" class="layer-content">
        <div class="layer-content__inner">

            <div class="amp-big-text">
                <div class="amp-big-text--content">21 janvier 2009</div>
            </div>


            <div class="amp-contexte right">
                <div class="amp-contexte--content">Barack Obama<br>devient le <b>premier<br>président<br>afro-américain</b><br>des États-Unis</div>
            </div>

        </div>
    </amp-story-grid-layer>

    <amp-story-grid-layer template="vertical" class="layer-foreground">
        <div class="layer-foreground__inner">
            <div class="amp-page-credits">
                <p>The Obama White House</p>
            </div>
        </div>
    </amp-story-grid-layer>
    
</amp-story-page>